CREATE TABLE apostador (
    id iINT PRIMARY KEY,
    nome varchar,
    contato INT(11),
    fk_Jogo_resultado_concurso_id integer
);

CREATE TABLE Jogo_resultado_concurso (
    valor Float,
    tipo varchar,
    numero INT,
    id integer,
    ID INT,
    jogo varchar,
    acerto INT,
    premio float,
    numero_sorteado INT,
    tipo_jogo varchar,
    data_sorteio date,
    PRIMARY KEY (id, ID)
);
 

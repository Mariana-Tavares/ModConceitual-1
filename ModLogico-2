CREATE TABLE Ligacao (
    ID INT PRIMARY KEY,
    data DATE,
    hora FLOAT,
    pulsos INT,
    Numero_discado INT(11),
    tipo_ligacao VARCHAR,
    minutos INT
);
CREATE TABLE Agenda (
    ID INT PRIMARY KEY,
    contato VARCHAR,
    numero INT(11),
    fk_Ligacao_ID INT
);
 
ALTER TABLE Agenda ADD CONSTRAINT FK_Agenda_2
    FOREIGN KEY (fk_Ligacao_ID)
    REFERENCES Ligacao (ID)
    ON DELETE RESTRICT; 
